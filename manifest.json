{
    "name": "Procrastinater Helper",
    "version": "1.0.0",
    "description": "This extension will cure you of the disease that is procrastination.",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistant": true
    }
}